<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="project.src.view.activities.LogsScreenActivity"
    tools:ignore="MissingDefaultResource">

    <View
        android:id="@+id/ipad_pro_2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/ipad_pro_11"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="355dp"
        android:layout_height="131dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ipad_pro_2"
        app:srcCompat="@drawable/logo_swarmbots"
        tools:ignore="ContentDescription" />

    <RelativeLayout
        android:id="@+id/frame_16"
        android:layout_width="1193dp"
        android:layout_height="512dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/frame_16"
        android:clipToOutline="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ipad_pro_2"
        app:layout_constraintVertical_bias="0.684" >

        <RelativeLayout
            android:id="@+id/frame_46"
            android:layout_width="260dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="933dp"
            android:layout_marginBottom="0dp"
            android:background="@drawable/frame_16"
            android:clipToOutline="true">

            <Button
                android:id="@+id/button"
                android:layout_width="248dp"
                android:layout_height="100dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="6dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="404dp"
                android:background="@drawable/logs_button_unselect"
                android:fontFamily="@font/inter_bold"
                android:onClick="onClickButtonIHM"
                android:text="IHM"
                android:textAppearance="@style/ihm" />

            <Button
                android:id="@+id/button2"
                android:layout_width="248dp"
                android:layout_height="100dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="6dp"
                android:layout_marginTop="112dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="300dp"
                android:background="@drawable/logs_button_unselect"
                android:fontFamily="@font/inter_bold"
                android:onClick="onClickButtonRobot1"
                android:text="ID: Robot1"
                android:textAppearance="@style/ihm" />

            <Button
                android:id="@+id/button3"
                android:layout_width="248dp"
                android:layout_height="100dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="6dp"
                android:layout_marginTop="216dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="196dp"
                android:background="@drawable/logs_button_unselect"
                android:fontFamily="@font/inter_bold"
                android:onClick="onClickButtonRobot2"
                android:text="ID: Robot2"
                android:textAppearance="@style/ihm" />
        </RelativeLayout>

        <ListView
            android:id="@+id/logs_list"
            android:layout_width="912dp"
            android:layout_height="492dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="271dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="12dp" />


    </RelativeLayout>

    <Button
        android:id="@+id/frame_68"
        android:layout_width="287dp"
        android:layout_height="71dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="40dp"
        android:background="@drawable/button"
        android:text="@string/export"
        android:textAppearance="@style/export"
        android:onClick="onClickExport"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.72"
        app:layout_constraintStart_toEndOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="parent">
    </Button>

    <Button
        android:id="@+id/frame_67"
        android:layout_width="287dp"
        android:layout_height="71dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="40dp"
        android:background="@drawable/button"
        android:onClick="onClickCommandScreenActivity"
        android:text="@string/commande"
        android:textAppearance="@style/commande"
        app:layout_constraintEnd_toStartOf="@+id/imageView9"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    </Button>

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="61dp"
        android:layout_height="61dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/ipad_pro_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/logo_swarmbots_a"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/logs"
        android:layout_width="198dp"
        android:layout_height="64dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="20dp"
        android:gravity="center_horizontal|top"
        android:text="@string/logs"
        android:textAppearance="@style/log"
        app:layout_constraintBottom_toBottomOf="@+id/ipad_pro_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ipad_pro_2"
        app:layout_constraintVertical_bias="0.155" />

</androidx.constraintlayout.widget.ConstraintLayout>